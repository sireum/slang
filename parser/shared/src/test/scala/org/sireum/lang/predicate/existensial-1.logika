(human(Socrates),  mortal(Socrates)) |- (∃{x => human(x) & mortal(x)}) Proof(
  1 #> (human(Socrates))                      by Premise,
  2 #> (mortal(Socrates))                     by Premise,
  3 #> (human(Socrates) & mortal(Socrates))   by AndI(1, 2),
  4 #> (∃{x => human(x) & mortal(x)})         by ExistsE(3, Socrates),
)