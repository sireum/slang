(vowel(e),  holds(square14, e)) |- (∃{(y, x) => vowel(y) & holds(x, y)}) Proof(
  1 #> (vowel(e))                              by "premise",
  2 #> (holds(square14, e))                    by "premise",
  3 #> (vowel(e) & holds(square14, e))         by "&i"(1, 2),
  4 #> (∃{(y, x) => vowel(y) & holds(x, y)})   by "∃i"(3, e, square14),
)