(∀{x => human(x) imply_: mortal(x)},  ∃{y => human(y)}) |- (∃{z => mortal(z)}) Proof(
  1 #> (∀{x => human(x) imply_: mortal(x)})   by "premise",
  2 #> (∃{y => human(y)})                     by "premise",
  3 #> Let { a => SubProof(
    4 #> Assume(human(a)),
    5 #> (human(a) imply_: mortal(a))         by "∀e"(1, a),
    6 #> (mortal(a))                          by "imply_:e"(6, 4),
    7 #> (∃{z => mortal(z)})                  by "∃i"(7, a),
  )},
  8 #> (∃{z => mortal(z)})                    by "∃e"(2, 3),
)