(∀{x => healthy(x) imply_: happy(x)}) |- ((∀{y => healthy(y)}) imply_: (∀{x => happy(x)})) Proof(
 1 #> (∀{x => healthy(x) imply_: happy(x)})               by "premise",
 2 #> SubProof(
   3 #> Assume(∀{y => healthy(y)}),
   4 #> Let { a => SubProof(
     5 #> (healthy(a))                                    by "∀e"(3, a),
     6 #> (healthy(a) imply_: happy(a))                   by "∀e"(1, a),
     7 #> (happy(a))                                      by "imply_:e"(6, 5),
   )},
   8 #> (∀{x => happy(x)})                                by "∀i"(4),
 ),
 9 #> ((∀{y => healthy(y)}) imply_: (∀{x => happy(x)}))   by "imply_:i"(2),
)